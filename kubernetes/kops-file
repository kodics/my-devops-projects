kops create cluster --name=prestashop.kodicsglobal.com \ 
--state=s3://vprofile-kop-states --zones=us-east-2a,us-east-2b \ 
--node-count=2 --node-size=t3.small --master-size=t3.medium --dns-zone=kubevpro.groophy.in \ 
--node-volume-size=8 --master-volume-size=8

kops update cluster --name prestashop.kodicsglobal.com --state=s3://vprofile-kop-states --yes --admin

kops delete cluster --name prestashop.kodicsglobal.com --state=s3://vprofile-kop-states --yes --admin